{
	"manifest_version": 2,
	"name": "维基搜索",
	"description": "This extension demonstrates a browser action with kittens.",
	"version": "1.0",
	"permissions": ["contextMenus",
	"http://*/*",
	"tabs"],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["menu.js"]
	},
	"icons": {
		"16": "icon.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	}
}